<div class="container">
  <div class="row">
    <div class="col-xs-6 col-xs-offset-3 col-md-4 col-md-offset-4 table">
      <h2>Favourite Notes</h2>
      <% @followings.pluck(:followee_id).each do |followee| %>
      <ul>
        <li><%= User.find(followee).email %></li>
        <% User.find(followee).notes.each do |notes| %>
          <li><%= notes.body %></li>
        <% end %>
      </ul>
      <% end %>
      <button type="button" class="btn btn-info active"><%= link_to 'Back', root_path %></button>
    </div>
  </div>
</div>
