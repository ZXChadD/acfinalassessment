<div class="container">
  <div class="row">
    <div class="col-xs-6 col-xs-offset-3 col-md-4 col-md-offset-4 table">
      <% unless current_user %>
        <h1>Users</h1>
        <% @users.each do |user| %>
        <ul>
          <li><%= link_to user.email, user_path(user) %></li>
        </ul>
        <% end %>
      <% else %>
        <h1>Followings</h1>
        <% @followings.pluck(:followee_id).each do |followee| %>
        <ul>
          <li><%= link_to User.find(followee).email, user_path(followee) %></li>
        </ul>
        <% end %>
      <% end %>
    </div>
  </div>
</div>
