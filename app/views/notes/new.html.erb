<div class="container">
  <div class="row">
    <div class="col-xs-6 col-xs-offset-3 col-md-4 col-md-offset-4 table">
      <h2>New Note</h2>
      <%= form_for @note do |f| %>
        <%= f.label :'Body' %>
        <%= f.text_field :body %>
      <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>
