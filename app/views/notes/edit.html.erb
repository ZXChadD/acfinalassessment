<div class="container">
  <div class="row">
    <div class="col-xs-6 col-xs-offset-3 col-md-6 col-md-offset-3">
      <h2>Edit Note</h2>
      <%= form_for @note do |f| %>
        <%= f.label :'Body' %>
        <%= f.text_field :body %>
      <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>
